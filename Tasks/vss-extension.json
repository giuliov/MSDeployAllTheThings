{
    "manifestVersion": 1,
    "id": "MSDeployAllTheThings",
    "name": "MSDeployAllTheThings",
    "version": "0.7.2",
    "publisher": "giuliovdev",
    "public": true,
    "targets": [
        {         
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Build/deployment tasks for MSDeploy.",
    "scopes": [
        "vso.build_execute",
        "vso.release_execute"
    ],
    "categories": [
        "Build and release"
    ],
    "icons": {
        "default": "images/MSDeployAllTheThingsIcon_128.png"        
    },
    "files": [
        {
            "path": "MSDeploySync",
            "addressable": false
        },
        {
            "path": "MSDeployDelete",
            "addressable": false
        }
    ],
    "screenshots": [
        {
            "path": "images/screenshot1.png"
        }
    ],
    "tags": [
        "msdeploy",
        "webdeploy"
    ],
    "content": {
        "details": {
            "path": "readme.md"
        }
    },
    "links": {
        "learn": {
            "uri": "https://github.com/giuliov/MSDeployAllTheThings/"
        }
    },
    "branding": {
        "theme": "light"
    },
    "contributions": [
        {
            "id": "msdeploy-sync",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "MSDeploySync"
            }
        },
        {
            "id": "msdeploy-delete",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "MSDeployDelete"
            }
        }
    ]

}
